#include <stdio.h>
#include <conio.h>
void main()
{
	int i,n;
	float pstv,ngtv,zero,pos=0,neg=0,num=0;
	scanf("%d",&n);
	int arr[n];
	for(i=0;i<n;i++)
	scanf("%d",&arr[i]);
	for(i=0;i<n;i++)
	if (arr[i]<0)
	{
	neg++;
	}
	
	else if(arr[i]>0)
	{
	pos++;
	}
	
	else
	num++;
	pstv=pos/n;
	ngtv=neg/n;
	zero=num/n;
	printf("%.6f\n%.6f\n%.6f\n",pstv,ngtv,zero);
		

	
}
